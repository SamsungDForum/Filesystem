<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns="http://www.w3.org/ns/widgets"
        xmlns:tizen="http://tizen.org/ns/widgets"
        id="http://yourdomain/Filesystem"
        version="1.1.0"
        viewmodes="maximized">
    <access origin="*" subdomains="true"></access>
    <tizen:application id="demoapp009.Filesystem"
                       package="demoapp009"
                       required_version="2.2"/>
    <content src="index.html"/>
    <feature name="http://tizen.org/feature/screen.size.all"/>
    <icon src="icon.png"/>
    <tizen:metadata key="http://samsung.com/tv/metadata/multiscreen.support" value="false"/>
    <name>Filesystem</name>

    <!-- public privileges -->
    <tizen:privilege name="http://developer.samsung.com/privilege/productinfo"/>
    <tizen:privilege name="http://tizen.org/privilege/filesystem.read"/>
    <tizen:privilege name="http://tizen.org/privilege/filesystem.write"/>
    <tizen:privilege name="http://tizen.org/privilege/mediacapture"/>
    <tizen:privilege name="http://tizen.org/privilege/tv.inputdevice"/>
    <tizen:privilege name="http://tizen.org/privilege/tv.window"/>
    <tizen:privilege name="http://tizen.org/privilege/websetting"/>
    <tizen:privilege name="http://tizen.org/privilege/content.read"/>
    <tizen:privilege name="http://tizen.org/privilege/content.write"/>
    <tizen:privilege name="http://tizen.org/privilege/contentmanager.write"/>

    <tizen:profile name="tv"/>
    <tizen:setting screen-orientation="landscape" background-support="disable" encryption="disable" hwkey-event="disable" pointing-device-support="disable"/>
</widget>
